  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration dev",
    "test": "jest --config ./jest.config.js",
    "prepare": "husky install",
    "lint": "ng lint",
    "semver": "npm --no-git-tag-version version",
    "build:local": "ng build --configuration local",
