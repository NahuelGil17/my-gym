    "build:ssr:dev": "ng build && ng run simply-solv-backoffice-fe:server",
    "build:ssr:prod": "npm run build:prod && ng run simply-solv-backoffice-fe:server:prod",
    "prerender": "ng run simply-solv-backoffice-fe:prerender",
    "compodoc:build": "compodoc -p tsconfig.doc.json",
    "compodoc:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "compodoc:serve": "compodoc -s"
